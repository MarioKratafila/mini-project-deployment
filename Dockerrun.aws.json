{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "kratafila/mini-frontend",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
        "hostPort": 80,
        "containerPort":80
        }
      ],
      "links": ["backend"]
    },
    {
      "name": "backend",
      "image": "kratafila/mini-backend",
      "hostname": "backend",
      "essential": true,
      "memory": 128
    }
  ]
}
